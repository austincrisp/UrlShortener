@model UrlShortener.Models.ApplicationUser

<!-- Page Header -->
<!-- Set your background image for this header on the line below. -->
<header class="intro-header" style="background-image: url(/img/home-bg.jpg)">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                <div class="site-heading">
                    <h1>@Model.UserName's</h1>
                    <hr class="small">
                    <span class="subheading">Liked Bookmarks</span>
                </div>
            </div>
        </div>
    </div>
</header>

<div class="container">
    <div class="row">
        <div class="box">
            <div class="col-lg-12 text-center">
                @foreach (var like in ViewBag.likedBookmarks)
                {
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">@like.Target.Title</h3>
                        </div>
                        <div class="panel-body">@like.Target.Description</div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>